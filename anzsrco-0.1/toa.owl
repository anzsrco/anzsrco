  <rdf:Description rdf:about="">
    <dc:source rdf:resource="http://www.abs.gov.au/AUSSTATS/abs@.nsf/allprimarymainfeatures/5D99AEA1DD8AA8E0CA2574180005421C"/>
    <rdfs:seeAlso rdf:resource="http://www.abs.gov.au/ausstats/abs@.nsf/mf/1297.0"/>
